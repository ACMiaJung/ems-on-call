@model acemsoncall.web.Models.EntityModel.EMSContent

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h6>Are you sure you want to delete this?</h6>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Content Type
        </dt>

        <dd>
            @Html.DisplayFor(model => model.contenttype)
        </dd>

        <dt>
            Title
        </dt>

        <dd>
            @Html.DisplayFor(model => model.title)
        </dd>

        <dt>
            Content
        </dt>

        <dd>
            @Html.Raw(Model.content)
        </dd>

        <dt>
            Date Registered
        </dt>

        <dd>
            @Html.DisplayFor(model => model.registdt)
        </dd>

        <dt>
            Date Updated
        </dt>

        <dd>
            @if (Model.updatedt != null)
            {
                @Html.DisplayFor(model => model.updatedt)
            }
            else
            {
                <p>None</p>
            }
        </dd>

        <dt>
            Registered By
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AspNetUser.MemberName)
        </dd>

        <dt>
            Last Updated By
        </dt>

        <dd>
            @if (Model.AspNetUser1 != null)
            {
                @Html.DisplayFor(model => model.AspNetUser1.MemberName)
            }
            else
            {
                <p>None</p>
            }
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
